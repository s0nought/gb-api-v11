{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "https://raw.githubusercontent.com/s0nought/gb-api-v11/main/schemas/base/_asubmitter.json",
    "title": "_aSubmitter",
    "description": "/base/_asubmitter",
    "type": "object",
    "properties": {
        "_idRow": {
            "type": "number",
            "exclusiveMinimum": 0
        },
        "_sName": {
            "type": "string",
            "minLength": 3,
            "maxLength": 30
        },
        "_bIsOnline": {
            "type": "boolean"
        },
        "_bHasRipe": {
            "type": "boolean"
        },
        "_sProfileUrl": {
            "type": "string",
            "format": "uri"
        },
        "_sAvatarUrl": {
            "type": "string",
            "format": "uri"
        },
        "_aClearanceLevels": {
            "type": "array",
            "items": {
                "type": "string"
            },
            "minItems": 1,
            "uniqueItems": true
        },
        "_sHdAvatarUrl": {
            "type": "string",
            "format": "uri"
        },
        "_sUpicUrl": {
            "type": "string",
            "format": "uri"
        },
        "_sSubjectShaperCssCode": {
            "type": "string"
        },
        "_sMoreByUrl": {
            "type": "string",
            "format": "uri"
        }
    },
    "required": [
        "_idRow",
        "_sName",
        "_bIsOnline",
        "_bHasRipe",
        "_sProfileUrl",
        "_sAvatarUrl"
    ]
}